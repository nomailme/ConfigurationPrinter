<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>ConfigurationPrinter</PackageId>
    <Version>0.1.0</Version>
    <Authors>Iskander Yarmukhametov</Authors>
    <description>Prints user configured AspNetCore options.</description>

    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Nomailme.ConfigurationPrinter</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyName>Nomailme.ConfigurationPrinter</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.1.1" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="2.1.1" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="2.1.1" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>


</Project>
